{
  "name": "qiime-studio-frontend",
  "version": "0.0.1",
  "description": "A web-based interface for QIIME 2.",
  "author": "QIIME 2 Development Team",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/qiime2/qiime-studio-backend.git"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint 'app/**/*.css' --config config/stylelint.yaml",
    "lint:js": "eslint app/ config/ --ext .js,.jsx --config config/eslint.yaml",
    "pretest": "npm run lint --silent",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "start": "webpack-dev-server --config config/webpack.dev.config.js --host localhost --port 4242 --inline",
    "build": "webpack --config config/webpack.prod.config.js"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "css-loader": "^0.23.1",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "mocha": "^2.4.5",
    "postcss-loader": "^0.8.2",
    "style-loader": "^0.13.1",
    "stylelint": "^5.2.1",
    "stylelint-config-standard": "^4.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "private": true
}
